<?xml version='1.0' encoding='utf-8'?>
<widget id="eu.wilkolek.amaze" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>A-Maze</name>
    <description>
        Sample maze game which will A-Maze you!
    </description>
    <author email="dwilkolek@protonmail.com" href="http://wilkolek.eu">
        Damian Wilko≈Çek
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-targetSdkVersion" value="25" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
</widget>
